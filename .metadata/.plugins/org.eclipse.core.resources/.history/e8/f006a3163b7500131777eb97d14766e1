package com.crgp.robot;

public abstract class BaseRobot {

	// -------------------------
	// ---
	// ---信号与槽(Signal and Slots)
	// ---
	// -------------------------
	/**
	 * [SLOT] 机器人名字
	 * 
	 * @return 机器人名字
	 */
	public abstract String name();

	/**
	 * [SLOT] 执行机器人代码
	 * 
	 * @param code
	 *            回调标识
	 * 
	 */
	public abstract void play(int code);

}
